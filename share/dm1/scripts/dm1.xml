<application>
<name>dm1 App</name>
    <module>
        <name>yarpdev</name>
        <parameters>--device OpenNI2DeviceServer --noMirror --depthVideoMode 4 --colorVideoMode 9</parameters>
        <node>/head</node>
    </module>

    <module>
        <name>cvFaces</name>
        <parameters>--kinectDevice OpenNI2DeviceClient --kinectLocal /cvFaces</parameters>
        <node>/head</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /faces/yarpview/img:i</parameters>
        <node>localhost</node>
    </module>
    <connection>
        <from>/cvFaces/img:o</from>
        <to>/faces/yarpview/img:i</to>
    </connection>

    <module>
        <name>sr1</name>
        <node>/head</node>
    </module>

    <module>
        <name>iSpeak</name>
        <node>/head</node>
    </module>

    <module>
        <name>launchManipulation</name>
        <parameters>--from launchManipulation-fakeArms.ini</parameters>
        <node>/manipulation</node>
    </module>
    <module>
        <name>ec1</name>
        <node>/manipulation</node>
    </module>

</application>

